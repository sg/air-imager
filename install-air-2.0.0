#!/bin/sh

VERSION="2.0.0"
if [ "${PERLTK_VER}z" = "z" ] ; then
	PERLTK_VER="804.028_502"
	PERLTK_ALT_VER="804.028502"
fi
#TK_SOURCE="http://www.cpan.org/modules/by-module/Tk"
TK_SOURCE="http://search.cpan.org/CPAN/authors/id/S/SR/SREZIC"
TK_FILE=Tk-$PERLTK_VER.tar.gz
if [ "${TEMP_DIR}z" = "z" ] ; then
	TEMP_DIR="/tmp"
fi
TK_DIR=$TEMP_DIR/Tk-$PERLTK_VER
if [ "${INSTALL_DIR}z" = "z" ]; then
	INSTALL_DIR="/usr/local"
fi
if [ "${FINAL_INSTALL_DIR}z" = "z" ]; then
	FINAL_INSTALL_DIR=$INSTALL_DIR
fi

ICON_DIR="$INSTALL_DIR/share/air/bitmaps"
LOG_DIR="$INSTALL_DIR/share/air/logs"
INSTALL_LOG="$TEMP_DIR/air-install.log"

echo "Install Log for AIR Version $VERSION" > $INSTALL_LOG
date >> $INSTALL_LOG
[ -f /etc/redhat-release ] && cat /etc/redhat-release >> $INSTALL_LOG
[ -f /etc/gentoo-release ] && cat /etc/gentoo-release >> $INSTALL_LOG
echo PERLTK_VER=$PERLTK_VER >> $INSTALL_LOG
echo TEMP_DIR=$TEMP_DIR >> $INSTALL_LOG
echo INSTALL_DIR=$INSTALL_DIR >> $INSTALL_LOG
echo FINAL_INSTALL_DIR=$FINAL_INSTALL_DIR >> $INSTALL_LOG
cat /proc/cpuinfo >> $INSTALL_LOG
free >> $INSTALL_LOG

# check for uudecode before we get too far...
echo "checking for uudecode:" >> $INSTALL_LOG
which uudecode >> $INSTALL_LOG 2>&1
if [ $? -ne 0 ] ; then
	echo ""
	echo "FATAL ERROR:"
	echo ""
	echo "The uudecode program could not be found on your system."  
	echo "This installer can't proceed without it.  uudecode is"
	echo "part of the sharutils package.  Please install sharutils"
	echo "from your install CD-ROMs or download the latest version"
	echo "from your distribution's update site."
	echo ""
	exit 1
fi

echo "perl is located at:" >> $INSTALL_LOG
which perl >> $INSTALL_LOG 2>&1
if [ $? -ne 0 ] ; then
	echo ""
	echo "FATAL ERROR:"
	echo ""
	echo "Perl doesn't appear to be installed.  Please install"
	echo "Perl from your install CD-ROMs or download the latest"
	echo "version from perl.org and install it."
	exit 1
else 	
	perl -V >> $INSTALL_LOG
fi

echo ""
echo "Automated Image & Restore (AIR) Auto-installer $VERSION" | tee -a $INSTALL_LOG
echo "----------------------------------------------------" | tee -a $INSTALL_LOG
echo ""
echo "This program will install files on your system that are"
echo "necessary to run AIR (Automated Image & Restore)."
echo ""

echo "Testing for perl:" >> $INSTALL_LOG
CUR_VER=`perl -e 'use Tk;print "$Tk::VERSION";' 2>> $INSTALL_LOG`
if [ $? -ne 0 ] ; then
	CUR_VER="0"
fi
if [ $CUR_VER = $PERLTK_VER ] || [ $CUR_VER = $PERLTK_ALT_VER ] ; then
	echo "Perl/Tk ($CUR_VER) is already installed on your system, so"
	echo "this installer will only update the other components of AIR."
	UPGRADE="yes"
else
	echo "Perl/Tk is not installed on your system or you have a version"
	echo "($CUR_VER) other than what is called for, so for the installation"
	echo "to proceed, it will have to be downloaded from the CPAN site on"
        echo "the Internet.  This means your system must be connected to a"
        echo "network with Internet access and properly configured.  If you"
        echo "can "surf the web" then you should be okay."
	echo ""
	echo "Once Perl/Tk is downloaded, it will automatically be compiled"
	echo "and installed."
	UPGRADE="no"
fi
echo "Perl/Tk version: $TK_VER" >> $INSTALL_LOG

if [ ! "$INTERACTIVE" = "no" ] ; then
	echo ""
	echo "If you are ready to proceed with the installation/upgrade,"
	echo "press ENTER."
	echo ""
	echo "To cancel, hit Control-C."

	read foo < /dev/tty  
fi

if [ $UPGRADE = "no" ] ; then
echo "Attempting to get the latest version of Perl/Tk..."

echo "Need to download Perl/Tk:" >> $INSTALL_LOG
echo "decoding snarf..." >> $INSTALL_LOG

sed 's/^X//' << 'SHAR_EOF' | uudecode -o $TEMP_DIR/snarf.gz 2>> $INSTALL_LOG
